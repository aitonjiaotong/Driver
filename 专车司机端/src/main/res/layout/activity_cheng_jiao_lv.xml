<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.aiton.bamin.driver.activity.ChengJiaoLvActivity">

    <RelativeLayout
        android:id="@+id/relativeLayout30"
        android:layout_width="match_parent"
        android:layout_height="@dimen/title_hight"
        android:background="@color/white"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true">

        <TextView
            android:id="@+id/textView137"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="成交率"
            android:textSize="@dimen/title_textsize"/>

        <ImageView
            android:id="@+id/imageView_back"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:paddingLeft="10dp"
            android:paddingRight="20dp"
            android:src="@mipmap/icon_back"/>
    </RelativeLayout>

    <LinearLayout
        android:orientation="vertical"
        android:background="@color/aiton_basic_color"
        android:layout_width="match_parent"
        android:layout_height="220dp"
        android:layout_below="@+id/relativeLayout30"
        android:layout_centerHorizontal="true"
        android:id="@+id/linearLayout2">

        <RelativeLayout
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <RelativeLayout
                android:background="@mipmap/chengjiaol2x"
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true">

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:id="@+id/relativeLayout31">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:textColor="@color/white"
                        android:textSize="50dp"
                        android:id="@+id/textView138"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"/>

                    <TextView
                        android:textColor="@color/white"
                        android:textSize="30dp"
                        android:layout_marginBottom="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="%"
                        android:id="@+id/textView139"
                        android:layout_alignBottom="@+id/textView138"
                        android:layout_toRightOf="@+id/textView138"
                        android:layout_toEndOf="@+id/textView138"/>
                </RelativeLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="接单成交率"
                    android:textColor="@color/white"
                    android:textSize="@dimen/normal_textsize"
                    android:id="@+id/textView140"
                    android:layout_below="@+id/relativeLayout31"
                    android:layout_centerHorizontal="true"/>
            </RelativeLayout>
        </RelativeLayout>

        <RelativeLayout
            android:layout_weight="5"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="2016-4-13"
                    android:textColor="@color/white"
                    android:textSize="@dimen/normal_textsize"
                    android:id="@+id/textView141"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true"/>

                <ImageView
                    android:src="@mipmap/you2x_white"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView45"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/textView141"
                    android:layout_toEndOf="@+id/textView141"
                    android:layout_marginLeft="20dp"
                    />

                <ImageView
                    android:src="@mipmap/zuo2x_white"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView46"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/textView141"
                    android:layout_toStartOf="@+id/textView141"
                    android:layout_marginRight="20dp"
                    />
            </RelativeLayout>
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_margin="25dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/linearLayout2"
        android:layout_centerHorizontal="true">

        <TextView
            android:layout_marginBottom="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="派单规则"
            android:textColor="@color/black"
            android:textSize="@dimen/normal_textsize"
            android:id="@+id/textView142"/>

        <TextView
            android:lineSpacingExtra="5dp"
            android:textSize="@dimen/normal_textsize"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1.每单得到的业务费用80%+用户加价100%+附加费用100%+其他费用80%。"
            android:id="@+id/textView143"/>

        <TextView
            android:lineSpacingExtra="5dp"
            android:layout_marginTop="5dp"
            android:textSize="@dimen/normal_textsize"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2.每单得到的业务费用80%+用户加价100%+附加费用100%+其他费用80%。"
            android:id="@+id/textView144"/>

        <TextView
            android:lineSpacingExtra="5dp"
            android:layout_marginTop="5dp"
            android:textSize="@dimen/normal_textsize"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3.每单得到的业务费用80%+用户加价100%+附加费用100%+其他费用80%。"
            android:id="@+id/textView145"/>
    </LinearLayout>
</RelativeLayout>
