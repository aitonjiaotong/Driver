<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.aiton.bamin.driver.activity.TiXianActivity">

    <RelativeLayout
        android:id="@+id/relativeLayout16"
        android:layout_width="match_parent"
        android:layout_height="@dimen/title_hight"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:background="@color/white">

        <TextView
            android:id="@+id/textView93"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="提现"
            android:textSize="@dimen/title_textsize"/>

        <ImageView
            android:id="@+id/imageView_back"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:paddingLeft="10dp"
            android:paddingRight="20dp"
            android:src="@mipmap/icon_back"/>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@+id/relativeLayout16"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:background="@color/white"
        android:id="@+id/relativeLayout17">

        <TextView
            android:id="@+id/textView101"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="为了保证你的财产安全，您只能使用"
            android:textSize="11dp"
            />

        <TextView
            android:id="@+id/textView102"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@+id/textView101"
            android:layout_toRightOf="@+id/textView101"
            android:text="本人本地"
            android:textColor="@color/aiton_basic_color"
            android:textSize="11dp"/>

        <TextView
            android:id="@+id/textView103"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@+id/textView102"
            android:layout_toRightOf="@+id/textView102"
            android:text="银行卡"
            android:textSize="11dp"/>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@+id/relativeLayout17"
        android:layout_centerHorizontal="true"
        android:id="@+id/relativeLayout18">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="账户姓名"
            android:textSize="@dimen/normal_textsize"
            android:id="@+id/textView104"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="15dp"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="张小闽"
            android:textSize="@dimen/normal_textsize"
            android:id="@+id/textView105"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/textView104"
            android:layout_toEndOf="@+id/textView104"
            android:layout_marginLeft="15dp"
            />

        <ImageView
            android:src="@mipmap/gantanhao2x"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView35"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="15dp"
            />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="@color/white"
        android:layout_below="@+id/relativeLayout18"
        android:layout_centerHorizontal="true"
        android:id="@+id/relativeLayout20">
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/relativeLayout19"
        android:layout_below="@+id/relativeLayout20"
        android:layout_centerHorizontal="true">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="提现账户"
            android:textSize="@dimen/normal_textsize"
            android:id="@+id/textView106"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="15dp"/>

        <EditText
            android:textSize="@dimen/normal_edittextsize"
            android:hint="请输入银行卡号"
            android:background="@null"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/editText"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/textView106"
            android:layout_toEndOf="@+id/textView106"
            android:layout_marginLeft="15dp"
            />

    </RelativeLayout>

    <RelativeLayout
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_below="@+id/relativeLayout19"
        android:layout_centerHorizontal="true"
        android:id="@+id/relativeLayout22">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true">

            <TextView
                android:textSize="11dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="为保证您的财产安全，每日提现上限"
                android:id="@+id/textView107"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"/>

            <TextView
                android:textColor="@color/aiton_basic_color"
                android:textSize="11dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="15000"
                android:id="@+id/textView108"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@+id/textView107"
                android:layout_toEndOf="@+id/textView107"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="元"
                android:textSize="11dp"
                android:id="@+id/textView109"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@+id/textView108"
                android:layout_toEndOf="@+id/textView108"/>

            <TextView
                android:textSize="11dp"
                android:textColor="@color/black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="可提现金额150.00元（本周收入下周提现请留\n余额供72小时后代充值）"
                android:id="@+id/textView110"
                android:layout_below="@+id/textView107"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"/>
        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/relativeLayout21"
        android:layout_below="@+id/relativeLayout22"
        android:layout_centerHorizontal="true">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="提现金额"
            android:textSize="@dimen/normal_textsize"
            android:id="@+id/textView111"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="15dp"/>

        <EditText
            android:textSize="@dimen/normal_edittextsize"
            android:hint="请输入银行卡号"
            android:background="@null"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/editText2"
            android:layout_toRightOf="@+id/textView111"
            android:layout_marginLeft="15dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:textSize="@dimen/normal_textsize"
            android:layout_height="wrap_content"
            android:text="元"
            android:id="@+id/textView112"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="15dp"
            />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="@color/white"
        android:id="@+id/relativeLayout23"
        android:layout_below="@+id/relativeLayout21"
        android:layout_centerHorizontal="true"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/relativeLayout24"
        android:layout_below="@+id/relativeLayout23"
        android:layout_centerHorizontal="true">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="登录密码"
            android:textSize="@dimen/normal_textsize"
            android:id="@+id/textView113"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="15dp"/>

        <EditText
            android:textSize="@dimen/normal_edittextsize"
            android:hint="请输入八闽出行登录密码"
            android:background="@null"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/editText3"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/textView113"
            android:layout_toEndOf="@+id/textView113"
            android:layout_marginLeft="15dp"/>
    </RelativeLayout>

    <RelativeLayout
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/relativeLayout24"
        android:layout_centerHorizontal="true">

        <Button
            style="@style/btn_style"
            android:enabled="false"
            android:layout_margin="20dp"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:text="确认提现"
            android:id="@+id/button"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"/>
    </RelativeLayout>
</RelativeLayout>
